y2=float(input("Coordenada y2: "))
x3=float(input("Coordenada x3: "))
y3=float(input("Coordenada y3: "))

lista=[(x1,y1),(x2,y2),(x3,y3)]

po=[(x2+x1)/2,(y2+y1)/2]
px1=(x2+x1)/2
py1=(y2+y1)/2

p1=[(px1+x3)/2,(py1+y3)/2]





print po,p1

